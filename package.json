{
    "name": "@capmeth/acid",
    "version": "0.0.1",
    "description": "A Component Interface Documenter",
    "title": "ACID",
    "author": "Ben Captison",
    "license": "MIT",
    "main": "main.js",
    "type": "module",
    "homepage": "https://capmeth.github.io/acid",
    "logo": "https://capmeth.github.io/acid/acid.png",
    "private": true,
    "scripts": {
        "bundle": "rollup -c docsite/rollup.config.js",
        "lint": "eslint -c lint/eslint.config.js",
        "test": "c8 -c test/c8.config.json ava --config test/ava.config.js"
    },
    "keywords": [
        "component",
        "documentation",
        "generator",
        "jsdoc",
        "markdown",
        "svelte",
        "ui",
        "workbench"
    ],
    "dependencies": {
        "@rollup/plugin-alias": "^5.1.1",
        "@rollup/plugin-commonjs": "^28.0.2",
        "@rollup/plugin-image": "^3.0.3",
        "@rollup/plugin-inject": "^5.0.5",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^16.0.0",
        "chalk": "^5.4.1",
        "change-case": "^5.4.4",
        "chokidar": "^4.0.3",
        "commander": "^13.0.0",
        "deepmerge": "^4.3.1",
        "glob": "^11.0.1",
        "js-yaml": "^4.1.0",
        "rollup": "^4.43.0",
        "rollup-plugin-analyzer": "^4.0.0",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-svelte": "^7.2.2",
        "svelte": "^5.34.7",
        "takedown": "^0.1.1",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "ava": "^6.4.0",
        "c8": "^10.1.3",
        "eslint": "^9.29.0",
        "eslint-plugin-svelte": "^3.9.2",
        "sinon": "^21.0.0"
    },
    "imports": {
        "#exts/*": "./extensions/*.js",
        "#lib/*": "./source/lib/*",
        "#packson": "./package.json",
        "#paths": "./paths.js",
        "#source/*": "./source/*",
        "#test/*": "./test/*",
        "#utils": "./source/utils/index.js"
    },
    "exports": {
        ".": "./source/entry.js"
    },
    "bin": {
        "acid": "./source/cli/main.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/capmeth/acid.git"
    }
}
