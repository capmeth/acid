{
    "name": "@capmeth/acid",
    "version": "0.0.1",
    "description": "Another Component Interface Documenter",
    "title": "ACID",
    "author": "Ben Captison",
    "license": "MIT",
    "main": "main.js",
    "type": "module",
    "private": true,
    "scripts": {
