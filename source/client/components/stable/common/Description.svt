<!--*
    Displays `value` as HTML content.

    @tags uses:main/Markup

    @property { string } value
      HTML description content.
    @property { string } type
      Indicates type of description.
-->

<Markup {type} empty={!value}>
  {@html value}
</Markup>


<script module>
import Markup from '#custom/main/Markup'
</script>

<script>
let { type, value } = $props();
</script>
